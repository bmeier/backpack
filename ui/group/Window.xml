<GuiXml>
	<Controls>
		<Control name="BackpackGroupWindowTitleBarBase" virtual="true">
			<Controls>
				<Label name="$(parent)TitelLabel">
					<Anchor point="TOPLEFT" relativePoint="TOPLEFT" relativeTo="$(parent)" />
				</Label>
			</Controls>
		</Control>

		<TopLevelControl name="BackpackGroupWindow" inherits="BackpackWindow" virtual="true">
			
			<Controls>
				<Control name="$(parent)TitleBar" inherits="BackpackGroupWindowTitleBarBase">
					<Anchor point="TOPLEFT" relativePoint="TOPLEFT" relativeTo="$(parent)" />
				</Control>
				<Control name="$(parent)ContentPanel">
					<Anchor point="TOPLEFT" relativePoint="BOTTOMLEFT"
						relativeTo="$(parent)NameLabel" />
				</Control>
			</Controls>
		</TopLevelControl>
	</Controls>
</GuiXml>